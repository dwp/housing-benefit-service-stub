          "type": "string",
          "description": "Claim Reference Number (SHBE field 2)",
          "pattern": "^[a-zA-Z0-9]{1,20}$"
        },
        "entitlements": {
          "type": "array",
          "description": "Entitlements",
          "items": {
            "$ref": "#/definitions/entitlement"
