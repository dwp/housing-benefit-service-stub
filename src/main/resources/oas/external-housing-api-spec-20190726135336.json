        "nino": {
          "type": "string",
          "description": "Claimant's National Insurance Number (SHBE field 4)",
          "pattern": "^((?!(BG|GB|KN|NK|NT|TN|ZZ)|([DFIUV])[A-Z]|[A-Z]([DFIOUV]))[A-Z]{2})[0-9]{6}[A-D]?$"
